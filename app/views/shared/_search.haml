-#Locals: search_results

- content_for :head do
  = javascript_include_tag 'taxa_autocomplete'

= form_tag catalog_search_path, method: :get do
  = hidden_field_tag 'id', params[:id]
  .search
    Search for name
    = search_selector params[:st]
    = text_field_tag 'qq', @search_query, class: 'typeahead', placeholder: "Search..."
    = submit_tag  'Go', class: 'submit'
    = image_tag 'ui-anim_basic_16x16.gif', class: 'throbber'
