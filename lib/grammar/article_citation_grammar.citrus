grammar ArticleCitationGrammar
  include PaginationGrammar

  rule article_citation
    (journal_name_series_volume_issue ':' space pagination) {
      {:journal_name_series_volume_issue => journal_name_series_volume_issue,
       :pagination => pagination}
    }
  end

  rule journal_name_series_volume_issue
    [A-Z] [^:]+
  end
end
